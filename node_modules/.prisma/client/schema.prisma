generator clinet{
    provider = "prisma-client-js"
}
datasource db{
    provider =  "mysql"
    url = env("DATABASE_URL")
    shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
}

model User {
  id      Int      @id @default(autoincrement())
  email   String   @unique
  studentid String @unique
  name String
  role    Role
  channel Int
  emailVerified Boolean?
}

enum Role{
   STUDENT
  TEACHER
  ADMIN
}